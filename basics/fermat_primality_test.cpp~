#include <bits/stdc++.h>
using namespace std;

int modulo(int a, int b, int m) {
	int r = 1;
	while(b) {
		if(b & 1) {
			r *= a;
			r %= m;
		}

		a *= a;
		a %= m;
		b /= 2;
	}

	return r;
}

bool fermat(int p, int itr = 4) {
	if(p == 1) return 0;
	
	while(itr--) {
		int a = rand() % (p-1) + 1;
		if(modulo(a, p, p-1) != 1) return false;	
	}

	return true;
}

int main() {
	for(;;) {
		int n;
		cin >> n;
		cout << fermat(n) << endl;
	}
	return 0;
}

